<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="panneauPrincipal" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuPrincipalController">
   <children>
      <BorderPane prefHeight="600.0" prefWidth="800.0">
         <center>
            <TabPane id="fenetrePrincipale" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
              <tabs>
                <Tab text="Pharmacie 1">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <FlowPane layoutX="-1.0" layoutY="-1.0" prefHeight="523.0" prefWidth="800.0">
                                 <children>
                                    <VBox prefHeight="200.0" prefWidth="307.0">
                                       <children>
                                          <Label text="Personnel" />
                                       </children>
                                    </VBox>
                                    <VBox prefHeight="194.0" prefWidth="493.0">
                                       <children>
                                          <Label text="Transactions" />
                                       </children>
                                    </VBox>
                                    <VBox layoutX="317.0" layoutY="10.0" prefHeight="323.0" prefWidth="801.0">
                                       <children>
                                          <Label text="Stocks" />
                                       </children>
                                    </VBox>
                                 </children>
                              </FlowPane>
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
                <Tab text="Pharmacie 2">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                  </content>
                </Tab>
                  <Tab text="Pharmacie 3">
                     <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                     </content>
                  </Tab>
              </tabs>
            </TabPane>
         </center>
         <top>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Label contentDisplay="CENTER" text="Gestionnaire des pharmacies">
                     <font>
                        <Font size="33.0" />
                     </font>
                  </Label>
                  <Button id="AjouteurPharmacie" alignment="BASELINE_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" text="Ajouter une nouvelle pharmacie" onAction="#ajouterPharmacie">
                     <HBox.margin>
                        <Insets left="100.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
